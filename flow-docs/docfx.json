{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "extendBreadcrumb": "true",
      "breadcrumb_path": "breadcrumb/toc.yml" 
     },
    "fileMetadata": {},
    "template": [],
    "dest": "flow"
  }
}